{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-await-in-loop": "off",
        "max-classes-per-file": "off",
        "no-nested-ternary": "off"
    }
}